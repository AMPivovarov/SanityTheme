{
  "name": "SanityTheme",
  "author": "JetBrains",
  "editorScheme": "/theme/Sanity.xml",
  "parentTheme": "ExperimentalLightWithLightHeader",
  "dark": false,
  "colors": {
  },
  "ui": {
    "*.selectionBackground": "Blue10",
    "*.lightSelectionBackground": "Blue10",
    "*.selectionInactiveBackground": "Gray11",

    "Menu.borderColor": "Gray11",
    "Borders.color": "Gray11",

    "ToolWindow.Header.borderColor": "Gray11",
    "ToolWindow.Button.selectedBackground": "Blue6",
    "EditorTabs.borderColor": "Gray11",
    "EditorTabs.unselectedBlend": "1.0",
    "EditorTabs.fontSizeOffset": 0,
    "EditorTabs.fontSizeOffset.compact": 0,

    "VersionControl": {
      "Log.Commit.selectionInactiveBackground": "Gray9",
      "Log.Commit.currentBranchBackground": "#ebe3eb",
      "Log.Commit.hoveredBackground": "Gray10",
      "Log.Commit.unmatchedForeground": "Gray02",
      "FileHistory.Commit.selectedBranchBackground": "#fffee4"
    },

    "comment": "Workaround for the LafManagerImplKt#patchHiDPI",
    "hidpi.scaleFactor": "1.0"
  }
}